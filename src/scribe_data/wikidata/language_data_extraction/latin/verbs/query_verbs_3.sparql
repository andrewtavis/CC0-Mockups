# tool: scribe-data
# All Latin (Q397) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?verb
  ?ThirdPersonPluralIndicativePassive
  ?ThirdPersonPluralIndicativeActive
  ?FirstPersonSingularIndicativePassive
  ?ThirdPersonSingularIndicativePassive
  ?SecondPersonSingularIndicativeActive
  ?ThirdPersonSingularIndicativeActive
  ?FirstPersonPluralIndicativePassive
  ?FirstPersonPluralIndicativeActive
  ?SecondPersonPluralIndicativePassive
  ?SecondPersonPluralActivePassive
  ?SecondPersonPluralImperativePassive

WHERE {
  ?lexeme dct:language wd:Q397 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb .

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?ThirdPersonPluralIndicativePassiveForm .
    ?ThirdPersonPluralIndicativePassiveForm ontolex:representation ?ThirdPersonPluralIndicativePassive ;
      wikibase:grammaticalFeature wd:Q51929074, wd:Q146786, wd:Q192613, wd:Q682111, wd:Q1194697 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?ThirdPersonPluralIndicativeActiveForm .
    ?ThirdPersonPluralIndicativeActiveForm ontolex:representation ?ThirdPersonPluralIndicativeActive ;
      wikibase:grammaticalFeature wd:Q51929074, wd:Q146786, wd:Q192613, wd:Q682111, wd:Q1317831 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?FirstPersonSingularIndicativePassiveForm .
    ?FirstPersonSingularIndicativePassiveForm ontolex:representation ?FirstPersonSingularIndicativePassive ;
      wikibase:grammaticalFeature wd:Q21714344, wd:Q110786, wd:Q192613, wd:Q682111, wd:Q1194697 .
